import java.util.*;
public class Print_n_from_1_to_n {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int i = 1;
        int n = sc.nextInt();
        while(i <= n){
          System.out.print(i + " ");
          i++;
        }
        System.out.println();
    }
}
