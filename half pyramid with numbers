import java.util.*;
public class Half_Pyramid_numbers {
    public static void halfPyramid(int n){
        for(int i=1; i<=n; i++){
            for(int j=1; j<=n-i+1; j++){
                System.out.print(j);
            }
            System.out.println();
        }
    }
    public static void main(String args[]){
        halfPyramid(5);
    }
}
